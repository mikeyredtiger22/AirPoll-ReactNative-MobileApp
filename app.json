{
  "expo": {
    "name": "AirPoll",
    "description": "React Native Expo mobile app. ECS air pollution research project.",
    "slug": "AirPoll",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.0",
    "orientation": "portrait",
    "primaryColor": "#00DEAD",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/title-background.png",
      "resizeMode": "cover",
      "backgroundColor": "#1DB2A5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#158CAE"
    }
  }
}
